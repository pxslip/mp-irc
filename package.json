{
    "name": "mp-irc",
    "version": "0.0.0",
    "license": "MIT",
    "angular-cli": {},
    "main": "main-src/main",
    "scripts": {
        "ng": "ng",
        "clean": "rm -rf dist/",
        "watch": "npm-run-all --parallel ng:watch tsc:watch",
        "build": "npm-run-all ng:build tsc:build",
        "ng:watch": "ng build -w",
        "ng:build": "ng build",
        "tsc:watch": "tsc -w -p main-src/tsconfig.main.app.json",
        "tsc:build": "tsc -p main-src/tsconfig.main.app.json",
        "electron:start": "electron dist/app/main/main.js",
        "start": "npm-run-all clean build --parallel electron:start watch",
        "lint": "tslint \"renderer-src/**/*.ts\" \"main-src/**/*.ts\"",
        "test": "ng test",
        "pree2e": "webdriver-manager update --standalone false --gecko false",
        "e2e": "protractor"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^4.0.0",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^11.0.5",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/platform-server": "^4.0.0",
        "@angular/router": "^4.0.0",
        "bootstrap": "^3.3.7",
        "core-js": "^2.4.1",
        "electron": "^1.4.13",
        "ng2-bootstrap": "^1.4.0",
        "rxjs": "^5.0.1",
        "ts-helpers": "^1.1.1",
        "zone.js": "^0.7.2"
    },
    "devDependencies": {
        "@angular/cli": "^1.0.0",
        "@angular/compiler-cli": "^4.0.0",
        "@types/electron": "^1.4.31",
        "@types/es6-promise": "^0.0.32",
        "@types/jasmine": "2.5.38",
        "@types/node": "^6.0.42",
        "codelyzer": "~2.0.0-beta.1",
        "eslint": "^3.12.2",
        "jasmine-core": "2.5.2",
        "jasmine-spec-reporter": "2.5.0",
        "karma": "1.2.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.0.2",
        "karma-remap-istanbul": "^0.2.1",
        "less": "^2.7.2",
        "npm-run-all": "^4.0.0",
        "protractor": "~4.0.13",
        "ts-node": "1.2.1",
        "tslint": "^4.0.2",
        "typescript": "^2.2.1"
    }
}
