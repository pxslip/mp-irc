{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "mp-irc"
    },
    "apps": [{
        "tsconfig": "tsconfig.renderer.app.json",
        "testTsconfig": "tsconfig.renderer.spec.json",
        "root": "renderer-src",
        "outDir": "dist/app/renderer",
        "assets": [
            "assets",
            "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "test": "test.ts",
        "prefix": "app",
        "styles": [
            "styles.less"
        ],
        "scripts": [],
        "environmentSource": "environments/environment.ts",
        "environments": {
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "less",
        "component": {
            "inlineTemplate": false,
            "spec": true,
            "inlineStyle": false
        }
    }
}