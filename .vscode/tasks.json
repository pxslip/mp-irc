{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "yarn",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [{
            "taskName": "clean",
            "args": ["run", "clean"]
        },
        {
            "taskName": "watch",
            "args": ["run", "watch"]
        },
        {
            "taskName": "start",
            "args": ["start"]
        },
        {
            "taskName": "build",
            "args": ["run", "build"]
        },
        {
            "taskName": "test",
            "args": ["run", "test"]
        }
    ]
}